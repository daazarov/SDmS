{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  "NServiceBus": {
    "ConnectionString": "host=localhost;username=guest;password=guest",
    "RabbitEndPoint": {
      "Name": "sdms.device-listener.host",
